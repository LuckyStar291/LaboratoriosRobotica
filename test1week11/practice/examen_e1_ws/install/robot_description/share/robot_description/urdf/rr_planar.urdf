<?xml version="1.0"?>
<robot name="robot_planar1">
    <!-- Definicion de materiales -->
        <!-- RGB 1-> TRANSPARENCIA -->
    <material name="blue">
        <color rgba="0 0 0.6 1"/>
    </material>
    <material name="red">
        <color rgba="0.5 0 0 1"/>
    </material>
    <material name="green">
        <color rgba="0 0.5 0 1"/>
    </material>

    <!-- Link base -->

    <link name="base_link1">
        <visual>
            <geometry>
                <cylinder length="0.3" radius="0.1">
                </cylinder>
            </geometry>
            <origin rpy="0 0 0" xyz="2 0 0.15"/>
            <material name="green"/>

        </visual>
    </link>

    <!-- Joint base a link1 -->
    <joint name="q11" type="revolute">
        <parent link="base_link"/>
        <child link="link1"/>
        <limit effort="300" velocity="0.1" lower="-3.1416" upper="3.1416"/>
        <axis xyz="0 0 1"/>
        <origin rpy="0 0 0" xyz="0 0 0.1" />
    </joint>

    <!-- Link eslabon 1 -->
    <link name="link11">
        <visual>
            <geometry>
                <box size="0.5 0.1 0.2"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0.25 0 0"/>
            <material name="blue"/>
        </visual>
    </link>

</robot>

<!-- Fin del robot planar RR -->
